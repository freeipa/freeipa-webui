{
  "name": "freeipa-webui",
  "version": "0.1.4",
  "homepage": ".",
  "private": true,
  "author": "",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@patternfly/patternfly": "^5.1.0",
    "@patternfly/react-core": "^5.1.2",
    "@patternfly/react-table": "^5.1.2",
    "@reduxjs/toolkit": "^2.6.1",
    "qrcode.react": "^4.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^6.30.0",
    "react-router-dom": "^6.30.0"
  },
  "scripts": {
    "dev": "npm run dev:prepare && vite",
    "dev:prepare": "rm -rf dist && mkdir dist && cp index.dev.html dist/index.html",
    "build": "tsc -b && vite build",
    "audit-ci": "audit-ci --config audit-ci.json",
    "cypress": "cypress run --config-file cypress/cypress.config.ts",
    "cypress:open": "cypress open --config-file cypress/cypress.config.ts",
    "lint": "eslint --color --max-warnings 0 src/",
    "lint:fix": "eslint --fix src/",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "cache:verify": "npm cache verify",
    "cache:clean": "npm cache clean --force"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@eslint/js": "^9.23.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.17",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.1.1",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "@vitest/coverage-v8": "^3.1.1",
    "audit-ci": "^7.1.0",
    "cy-verify-downloads": "^0.2.6",
    "cypress": "^14.2.1",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.4",
    "vitest": "^3.1.1"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "tests/features/steps/*.{js,ts}"
    ]
  },
  "engines": {
    "node": ">=18"
  }
}
